@model IEnumerable<ServerModel.Tag>

@{
    ViewBag.Title = "GetTags";
}

<h3>Tags</h3>
<table class="table table-hover" id="serviceTable">
    <thead>
        <tr>
            <th>Title</th>

        </tr>
    </thead>
    <tbody id="fbody">
        @foreach (var tag in Model)
        { 
            <tr>
                <td>@tag.title</td>

            </tr>
        }
    </tbody>
</table>
